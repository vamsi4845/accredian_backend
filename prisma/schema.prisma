datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Referral {
  id            Int      @id @default(autoincrement())
  name          String
  email         String
  friendsName  String
  friendsEmail String
  course        String
  createdAt     DateTime @default(now())
}